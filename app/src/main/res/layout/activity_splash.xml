<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:id="@+id/biometric_login"
    tools:context=".Splash">
    <ImageView

        android:id="@+id/icon_text1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="4dp"
        android:src="@mipmap/rr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.12"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.20"
        tools:ignore="MissingConstraints" />

    <ImageView

        android:id="@+id/icon_text2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.20"
        app:layout_constraintHeight_percent="0.12"
        android:src="@mipmap/aa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_text1"
        tools:ignore="MissingConstraints" />

    <ImageView

        android:id="@+id/icon_text3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.20"
        app:layout_constraintHeight_percent="0.12"
        android:src="@mipmap/ii"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_text2"
        tools:ignore="MissingConstraints" />

    <ImageView

        android:id="@+id/icon_text4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.20"
        app:layout_constraintHeight_percent="0.12"
        android:layout_marginTop="1dp"
        android:src="@mipmap/hh"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_text3"
        tools:ignore="MissingConstraints" />

    <ImageView

        android:id="@+id/icon_text5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.20"
        app:layout_constraintHeight_percent="0.12"
        android:src="@mipmap/ee"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/icon_text4"
        tools:ignore="MissingConstraints" />

    <ImageView

        android:id="@+id/icon_text6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:src="@mipmap/nn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.12"
        app:layout_constraintTop_toBottomOf="@id/icon_text5"
        app:layout_constraintWidth_percent="0.20"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>